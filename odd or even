package com.company;
import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        System.out.println("Enter n:");
	    Scanner scan=new Scanner(System.in);
	    int n=scan.nextInt();
	    scan.close();
	    if (n%2==0)
	        System.out.println(n+"is a even number");
	    else
	        System.out.println(n+"is a odd number");
    }
}
