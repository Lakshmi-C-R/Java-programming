package com.company;
import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
	    System.out.print("Enter m:");
	    Scanner x=new Scanner(System.in);
	    int m=x.nextInt();
	    System.out.print("Enter n:");
	    Scanner y=new Scanner(System.in);
	    int n=y.nextInt();
	    while(n!=0)
        {
            int r=m%n;
            m=n;
            n=r;
        }
	    System.out.println("GCD="+m);
    }
}
