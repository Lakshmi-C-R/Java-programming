import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
	System.out.print("Enter n:");
	Scanner scan=new Scanner(System.in);
	int n=scan.nextInt();
	int rev=0,d;
	int temp=n;
	while (n!=0)
    {
        d=n%10;
        rev=rev*10+d;
        n=n/10;
    }
	System.out.println("The reverse of "+temp+" is="+rev);
    }
}
