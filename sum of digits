import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
	System.out.print("Enter n:");
	Scanner scan=new Scanner(System.in);
	int n=scan.nextInt();
	int sum=0,d;
	while (n!=0)
    {
        d=n%10;
        sum=sum+d;
        n=n/10;
    }
	System.out.println("Sum of digits="+sum);
	    }
}
